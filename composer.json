{
    "name": "noopable/np-app",
    "description": "Noopable's base application module",
    "type": "np-local-module",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2", "noopable"
    ],
    "authors": [
        {
            "name": "Tomoaki Kosugi",
            "email": "kipspro@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/noopable/np-composer-installer.git"
        },
        {
            "type": "git",
            "url": "https://github.com/noopable/Flower.git"
        },
        {
            "type": "git",
            "url": "https://github.com/noopable/np-document.git"
        },
        {
            "type": "git",
            "url": "https://github.com/noopable/page.git"
        }
    ],
    "require": {
        "php": ">=5.4.10",
        "zendframework/zendframework": "dev-master",
        "noopable/np-composer-installer": "dev-master",
        "noopable/flower": "dev-master",
        "noopable/page": "dev-master",
        "noopable/np-document": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "NpApp\\": "src/",
            "NpAppTest\\": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}